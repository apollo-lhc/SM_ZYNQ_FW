STRING build_date$, "2023-07-08 22:20:40.878218"
str_build_date:
	LOAD&RETURN s1, build_date$
	LOAD&RETURN s1, 00
CMD_print_build_date:
	
	;; print banner
	LOAD sB,str_build_date'upper
	LOAD sA,str_build_date'lower
	CALL util_print_string

	;; print end of line
	CALL util_print_EOL

	RETURN
